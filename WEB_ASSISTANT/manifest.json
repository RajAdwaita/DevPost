{
    "manifest_version" : 2,
    "name" : "Web Assistant Extension",
    "version" : "1.0",
    "content_scripts" : [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : ["content.js"]
        }
    ],

    "permissions":["tabs" , "storage"],
    "background" : {
        "scripts" : ["background.js"]
    },
    "browser_action" : {
        "default_icon" : "images/extension_icon.png",
        "default_popup" : "styles/popup.html",
        "default_title" : "Search a Keyword."
    }
}